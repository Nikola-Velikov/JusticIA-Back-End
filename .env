# Copy to .env and fill in values

NODE_ENV=development
PORT=8081
# Example: mongodb://localhost:27017/jisticia
MONGO_URI=mongodb://localhost:27017/jisticia

# CORS allowed origin(s). Use * for all, or a single origin like http://localhost:3000
CORS_ORIGIN=*

# JWT secret for signing auth tokens
JWT_SECRET=change_me_in_production
